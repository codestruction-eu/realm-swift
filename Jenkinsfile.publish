pipeline {
  agent { label 'osx' }
  stages {
    stage('Gather Info') {
      agent { label 'osx' }
      steps {
        withAWS(credentials: 'cocoa-docs-s3-bucket', region: 'us-east-1') {
          sh 's3cmd get --access_key=${AWS_ACCESS_KEY_ID} --secret_key=${AWS_SECRET_ACCESS_KEY} s3://static.realm.io/update/cocoa'
        }
      }
    }
  }
}
